[openvswitch-xenial:vars]
ansible_ssh_user=ubuntu
ansible_become=true

[vyos-1.1.7:vars]
ansible_network_os=vyos
ansible_ssh_user=vyos
ansible_ssh_pass=vyos
ansible_connection=local

[veos-4.18.1F:vars]
ansible_network_os=eos
ansible_ssh_user=admin
ansible_ssh_pass=admin
ansible_connection=local

[junos-vqfx:vars]
ansible_network_os=junos
ansible_ssh_user=root
ansible_ssh_pass=Juniper
ansible_connection=local

[nxos-7.0.3.I5.1:vars]
ansible_connection=local
ansible_ssh_user=admin
ansible_ssh_pass=admin
ansible_ssh_port=8022
ansible_ssh_host=localhost
ansible_network_os=nxos
ansible_ssh_common_args='-o PubkeyAuthentication=no -o ProxyCommand="ssh -l fedora -W %h:%p {{ inventory_hostname }}"'

[net-vyos-1.1.7:vars]
ansible_network_os=vyos
ansible_ssh_user=vyos
ansible_ssh_pass=vyos
ansible_connection=local

[net-veos-4.18.1F:vars]
ansible_network_os=eos
ansible_ssh_user=admin
ansible_ssh_pass=admin
ansible_connection=local

[net-junos-vqfx:vars]
ansible_network_os=junos
ansible_ssh_user=root
ansible_ssh_pass=Juniper
ansible_connection=local

